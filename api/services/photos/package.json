    "serverless-bundle": "^4.1.0",
    "serverless-dotenv-plugin": "^3.1.0",
    "serverless-offline": "^6.8.0",
    "sharp": "^0.26.3"
  },
  "dependencies": {
    "axios": "^0.21.0",
    "body-parser": "^1.19.0",
    "serverless-express": "^2.0.11",
    "serverless-headless-google-oauth": "^0.1.0"
  }
